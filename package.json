{
  "name": "nuxt-envalid",
  "version": "0.0.3",
  "description": "A Nuxt.js module thats validates your env variables and loads them cleaned into your application context",
  "keywords": [
    "nuxt-module",
    "javascript",
    "envalid"
  ],
  "bugs": {
    "url": "https://github.com/manuelhenke/nuxt-envalid/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manuelhenke/nuxt-envalid.git"
  },
  "license": "MIT",
  "author": {
    "name": "Manuel Henke",
    "email": "contact@henkebyte.com",
    "url": "https://henkebyte.com/"
  },
  "main": "./dist/module.js",
  "types": "./dist/module.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "siroc build",
    "dev": "nuxt dev test/fixture/basic",
    "lint": "yarn lint:js && yarn lint:prettier",
    "lint:js": "eslint --ext \".js,.ts\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check --ignore-path .gitignore . '!CHANGELOG.md'",
    "lintfix": "yarn lint:js --fix && prettier --write --list-different --ignore-path .gitignore . '!CHANGELOG.md'",
    "prepare": "husky install",
    "prepublishOnly": "yarn build",
    "test": "jest --coverage"
  },
  "dependencies": {
    "envalid": "^7.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@nuxt/test-utils": "^0.2.2",
    "@nuxt/types": "^2.15.8",
    "@nuxtjs/eslint-config-typescript": "^10.0.0",
    "@types/jest": "^28.1.7",
    "@types/node": "^16.11.56",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.15.0",
    "husky": "^8.0.1",
    "jest": "^26.0.20s",
    "nuxt": "^2.15.8",
    "prettier": "^2.7.1",
    "siroc": "^0.16.0",
    "typescript": "^4.7.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
